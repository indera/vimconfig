XTerm*termName: xterm-256color
XTerm*faceName: Meslo LG S for Powerline
XTerm*faceSize: 9.2
XTerm*metaSendsEscape: false
XTerm*altSendsEscape: false
XTerm*reverseVideo: false
XTerm*toolBar: false
XTerm*VT100.cutNewLine: false
XTerm*VT100.cutToBeginningOfLine: false
XTerm*VT100.charClass: 33:48,35:48,37:48,42:48,45-47:48,64:48,95:48,126:48
XTerm*cursorBlink: true
XTerm*visualBell: true
XTerm*visualBellDelay: 0
XTerm*bellIsUrgent: true
XTerm*scrollBar: false

*allowWindowOps: true
*allowTitleOps: true
*allowFontOps: true
*allowTcapOps: true

! Solarized dark
XTerm*background:             #012833
XTerm*foreground:             #839496
XTerm*fading:                 40
XTerm*fadeColor:              #012833
XTerm*cursorColor:            #93a1a1
XTerm*pointerColorBackground: #586e75
XTerm*pointerColorForeground: #93a1a1
XTerm*color0:                 #023540
XTerm*color1:                 #dc322f
XTerm*color2:                 #859900
XTerm*color3:                 #b58900
XTerm*color4:                 #268bd2
XTerm*color5:                 #d33682
XTerm*color6:                 #2aa198
XTerm*color7:                 #eee8d5
XTerm*color8:                 #012833
XTerm*color9:                 #cb4b16
XTerm*color10:                #586e75
XTerm*color11:                #657b83
XTerm*color12:                #839496
XTerm*color13:                #6c71c4
XTerm*color14:                #93a1a1
XTerm*color15:                #fdf6e3

! ! Solarized light
! XTerm*background:              #fdf6e3
! XTerm*foreground:              #657b83
! XTerm*fadeColor:               #fdf6e3
! XTerm*cursorColor:             #586e75
! XTerm*pointerColorBackground:  #93a1a1
! XTerm*pointerColorForeground:  #586e75

! Solarized red cursor
XTerm*cursorColor: #dc322f

*VT100.translations: #override \n\
Meta <Key>1: string(0x1b) string("1") \n\
Meta <Key>2: string(0x1b) string("2") \n\
Meta <Key>3: string(0x1b) string("3") \n\
Meta <Key>4: string(0x1b) string("4") \n\
Meta <Key>5: string(0x1b) string("5") \n\
Ctrl <Key>0: string("0") \n\
Ctrl <Key>1: string("1") \n\
Ctrl <Key>2: string("2") \n\
Ctrl <Key>3: string("3") \n\
Ctrl <Key>4: string("4") \n\
Ctrl <Key>5: string("5") \n\
Ctrl <Key>6: string("6") \n\
Ctrl <Key>7: string("7") \n\
Ctrl <Key>8: string("8") \n\
Ctrl <Key>9: string("9") \n\
Ctrl ~Shift <Key>Tab: string(0x1b) string("[27;5;9~") \n\
Ctrl Shift <Key>Tab: string(0x1b) string("[27;6;9~") \n\
Ctrl ~Shift <Key>`: string(0x1e) \n\
Ctrl Shift <Key>`: string(0x1e) \n\
Ctrl ~Shift <Key>-: string(0x1f) \n\
Ctrl Shift <Key>-: string(0x1f) \n\
Meta <Key>-: smaller-vt-font() \n\
Meta <Key>+: larger-vt-font() \n

! vim: cms=!%s
